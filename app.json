{
  "expo": {
    "name": "Zapp",
    "slug": "zapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#10b981"
    },
    "platforms": [
      "ios",
      "android"
    ],
    "sdkVersion": "51.0.0",
    "android": {
      "package": "com.rasheed.zapp",
      "versionCode": 1,
      "googleServicesFile": "./android/app/google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#10b981"
      },
      "permissions": [
        "android.permission.CALL_PHONE",
        "android.permission.READ_CONTACTS"
      ]
    },
    "jsEngine": "hermes",
    "plugins": [
      "expo-font",
      "expo-localization",
      "@react-native-firebase/app"
    ],
    "extra": {
      "eas": {
        "projectId": "586efe5d-def5-417d-9203-c93032704afd"
      },
      "firebaseProject": "swift-payments-6492e"
    }
  }
}
